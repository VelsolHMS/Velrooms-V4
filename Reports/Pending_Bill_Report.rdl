  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT PendingBill_ID,Room_No,(SELECT RESERVATION_ID FROM CHECKIN WHERE ROOM_NO=PB.Room_No AND FIRSTNAME=PB.Guest_Name) AS RESERVATION_NO,
Guest_Name,(SELECT CONTACT_PERSON_NUMBER FROM COMPANYMASTER WHERE COMPANY_NAME=PB.Company_Name) AS CONTACT_NO,
(SELECT ADDRESS+' ,'+CITY+' ,'+STATE+' ,'+CAST(ZIP AS VARCHAR(20)) FROM CHECKIN WHERE FIRSTNAME=PB.Guest_Name AND COMPANY_NAME=PB.Company_Name) AS ADDRESS,
Company_Name,Amount_Recevied,Pending_Amount,
(SELECT CREDITLIMIT FROM COMPANYMASTER WHERE COMPANY_NAME=PB.Company_Name)-PB.Amount_Recevied AS BALANCE_AMOUNT 
FROM PENDINGBILL PB </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
