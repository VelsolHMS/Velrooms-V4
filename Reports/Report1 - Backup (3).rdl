        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select DISTINCT a.ROOM_NO,a.RESERVATION_ID,(select FIRSTNAME from CHECKIN where RESERVATION_ID !=0 and ROOM_NO=a.ROOM_NO) as FIRSTNAME,(select MOBILE_NO from CHECKIN where RESERVATION_ID !=0 and ROOM_NO=a.ROOM_NO) AS MOBILE_NO,(select ID_TYPE from CHECKIN where RESERVATION_ID !=0 and ROOM_NO=a.ROOM_NO) AS ID_TYPE, (select ID_DATA from CHECKIN where RESERVATION_ID !=0 and ROOM_NO=a.ROOM_NO) AS ID_DATA,a.AMOUNT_RECEIVED from ADVANCE a,CHECKIN b where a.INSERT_DATE=@DATE  and b.CHECK_OUT=0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@DATE">
            <Value>=Parameters!DATE.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
