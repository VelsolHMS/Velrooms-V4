      <Query>
        <DataSourceName>DataSource(HMS)</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT A.CANCEL_ID,A.RESERVATION_ID,A.GUESTNAME,A.MOBILE_NO,B.ID_TYPE,B.ID_DATA FROM RESERVATIONCANCEL A,RESERVATION B WHERE  B.STATUS='CANCLED' AND MONTH(B.INSERT_DATE)=@DATE</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@DATE">
            <Value>=Parameters!DATE.Value</Value>
          </QueryParameter>
