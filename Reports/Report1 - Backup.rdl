      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT A.ROOM_NO,A.CHECKIN_ID,A.RESERVATION_NO,A.GUEST_NAME,C.MOBILE_NO,B.TARRIF,B.TOTAL,A.AMOUNT AS DISCOUNT,C.ROOM_CATEGORY,DATEDIFF(DAY, B.ARRIVAL_DATE, B.DEPARTURE_DATE) AS TOTALDAYS FROM DISCOUNT A,CHECKOUT B,CHECKIN C WHERE  A.INSERT_DATE=@Date</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Date">
            <Value>=Parameters!Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
