      <Query>
        <DataSourceName>DataSourcehms</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT A.ROOM_NO,A.GUEST_NAME,(SELECT ADDRESS FROM CHECKIN WHERE ROOM_NO=A.ROOM_NO AND CHECK_OUT=0) AS GUESTADDRESS FROM CHECKOUT A,CHECKIN B,PRINTS C WHERE A.ROOM_NO=@ROOM_NO AND B.CHECK_OUT=0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ROOM_NO">
            <Value>=Parameters!ROOM_NO.Value</Value>
          </QueryParameter>
        </QueryParameters>
